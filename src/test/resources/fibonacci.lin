// A simple, functional style, fibonacci function written in Lintilla.
//

fn fibonacci(n : int) -> int {
  fn fib(res1 : int, res2 : int, count : int) -> int {
    if (0 < count) {
      fib(res2, res1 + res2, count - 1)
    } else {
      res1
    }
  };

  if (0 < n ) { fib(0, 1, n) } else { -1 }
};

print(fibonacci(10))